# vim: syntax=python

i = 0
while i < 10: # executes the body 10 times
  print(i + "\n")
  i = i + 1

print("\n-----------------------------------------------------\n\n")

i = 5
while true:
  print(i + "\n")
  i = i - 1
  if i == -1: break

print("\n-----------------------------------------------------\n\n")

while true:
  while true:
    print("Break 1\n")
    break
  print("Break 2\n")
  break
